{
  "name" : "sunglasses",
  "version" : "0.1",
  "description" : "Tints webpages so it doesn't hurt your eyes",
  "browser_action" : {
    "default_icon" : "icon.png",
    "popup" : "popup.html"
  },

  "content_scripts" : [
    {
      "matches" : ["http://*/*", "https://*/*"],
      "css" : ["sunglasses.css"],
      "js" : ["sunglasses.js"]
    }
  ]
}
